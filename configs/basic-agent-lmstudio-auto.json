{
  "identity": {
    "id": "basic-agent-lmstudio-auto",
    "name": "Basic Agent (LM Studio Auto)",
    "description": "Minimal CLI agent pre-configured for LM Studio local inference",
    "version": "0.1.0"
  },
  "cognition": {
    "provider": "lmstudio",
    "model": "llama-3.2-1b-instruct",
    "temperature": 0.7,
    "maxTokens": 8192,
    "maxToolRounds": 10
  },
  "capabilities": {
    "tools": ["fs.read", "fs.write", "fs.list", "fs.mkdir", "fs.delete"],
    "allowedPaths": ["."]
  },
  "policy": {
    "maxConcurrentTools": 1,
    "toolTimeout": 30000
  },
  "memory": {
    "slidingWindowSize": 5
  },
  "plugins": [
    { "name": "@openstarry-plugin/provider-lmstudio", "config": { "baseUrl": "http://127.0.0.1:1234/v1", "autoConnect": true } },
    { "name": "@openstarry-plugin/provider-gemini-oauth" },
    { "name": "@openstarry-plugin/standard-model-selector" },
    { "name": "@openstarry-plugin/standard-core-commands" },
    { "name": "@openstarry-plugin/standard-function-fs" },
    { "name": "@openstarry-plugin/standard-function-stdio" },
    { "name": "@openstarry-plugin/guide-character-init" }
  ],
  "guide": "default-guide"
}
